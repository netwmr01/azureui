{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "version":{
      "type":"object"
    },
    "networkSpec": {
      "type": "object"
    },
    "asName": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "apiVersion": "[parameters('version').resourceAPIVersion']",
